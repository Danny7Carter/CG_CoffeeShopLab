@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Welcome to CG Coffee Shop</h1>


</div>

<table>
   <tr>
       <th>Name</th>
       <th>Description</th>
       <th>Quantity</th>
       <th>Price</th>
   </tr>
   @for (int i = 0; i < ViewBag.Items.Count; i++)
   {
       <tr>
           <td>@ViewBag.Items[i].Name</td>
           <td>@ViewBag.Items[i].Description</td>
           <td>@ViewBag.Items[i].Quantity</td>
           <td>@ViewBag.Items[i].Price</td>
       </tr>
   }

</table>
<form action="/Home/Welcome">
   <input type="text" name="Description" />
   <input type="submit" value="Add Item" />
</form>